{
  "name": "cvttweb",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:fancybuild": "vite",
    "test": "vitest",
    "smoke": "playwright test tests/smoke.spec.ts --project=chromium --workers=1 --retries=1 --reporter=list",
    "smoke:mobile": "playwright test tests/smoke.mobile.spec.ts --project=chromium --workers=1 --retries=1 --reporter=list",
    "e2e": "playwright test tests/e2e --project=chromium --workers=1 --retries=1 --reporter=list",
    "test:coverage": "vitest --run --coverage \"src/**/*.test.ts\"",
    "build": "tsc -b && vite build",
    "build:fancybuild": "tsc -b && vite build",
    "build:gm-local": "tsc -b && vite build --config vite.gm.config.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.11",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.5",
    "vitest": "^1.6.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/coverage-istanbul": "^1.6.1",
    "@playwright/test": "^1.40.0"
  }
}
